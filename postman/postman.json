              "application/json; charset=utf-8": {
                "data": {
                  "id": "5d3b23b155f500465c895f60",
                  "status": "queued"
                }
              }
            },
            "headers": {
              "Content-Length": {
                "type": "string",
                "default": "60"
              },
              "Date": {
                "type": "string",
                "default": "Fri, 26 Jul 2019 16:00:49 GMT"
              },
              "Connection": {
                "type": "string",
                "default": "keep-alive"
              }
            }
          }
        }
      }
