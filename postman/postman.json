            "type": "string",
            "description": ""
          },
          {
            "name": "x-api-key",
            "in": "header",
            "required": true,
            "type": "string",
            "description": "API Key for your Atomic account"
          },
          {
            "name": "x-api-secret",
            "in": "header",
            "required": true,
            "type": "string",
            "description": "API Secret for your Atomic account"
          },
          {
            "name": "Body",
            "in": "body",
            "required": true,
            "description": "",
            "schema": {
              "$ref": "#/definitions/CreateaccesstokenRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "$ref": "#/definitions/Createaccesstokenresponse"
            },
            "examples": {
              "application/json": {
                "data": {
                  "publicToken": "6e93549e-3571-4f57-b0f7-77b7cb0b5e48",
